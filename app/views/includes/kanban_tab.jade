div(style="margin-top: 158px;")
div(class="navbar-fixed-top kanban-board-header")
    div(class="row kanban-board-row kanban-board-row-header")
        div(class="kanban-col-header")
            div(class="kanban-col-header-text") Backlog
        div(class="kanban-col-header")
            div(class="kanban-col-header-text") Selected for Development
        div(class="kanban-col-header")
            div(class="kanban-col-header-text") In Progress
        div(class="kanban-col-header")
            div(class="kanban-col-header-text") Completed
div(class="container-fluid kanban-board")
    div(class="row kanban-board-row")
        div(class="kanban-col")
            each task in taskList
                div(class="kanban-task")
                    a(href="#" class="kanban-task-header") #{task.taskid+' '+task.taskname}
                    div(class="kanban-task-body")
                        | #{task.taskdescription}
                    div(class="kanban-task-tags") Issue Priority: #{task.priority}
                    div(class="kanban-task-tags") Assigned To: #{task.assignedto}
        div(class="kanban-col")
        div(class="kanban-col")
        div(class="kanban-col")
            
